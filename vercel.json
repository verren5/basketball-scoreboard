{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/public/display.html"
      },
      {
        "src": "/control",
        "dest": "/public/control.html"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ]
  }
  